{
  "name": "on-budget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@carbon/icons-react": "10.22.0",
    "@loadable/component": "5.14.1",
    "@sentry/react": "5.29.2",
    "@sentry/tracing": "5.29.2",
    "@sentry/webpack-plugin": "1.14.0",
    "carbon-components": "10.25.0",
    "carbon-components-react": "7.25.0",
    "carbon-icons": "7.0.7",
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "clone-deep": "4.0.1",
    "connected-react-router": "6.8.0",
    "date-fns": "2.16.1",
    "firebase": "8.2.1",
    "firebase-functions": "3.13.0",
    "history": "4.10.1",
    "mkdirp": "1.0.4",
    "node-sass": "7.0.0",
    "papaparse": "5.3.0",
    "path": "0.12.7",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-redux-firebase": "3.8.1",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "redux": "4.0.5",
    "redux-firestore": "0.14.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "uuid": "8.3.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "start:emulate": "REACT_APP_EMULATING=true react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "firebase": "firebase emulators:start",
    "firebase:deploy": "firebase deploy --only functions",
    "firebase:populate": "firebase emulators:start --export-on-exit=./firebase --import=./firebase",
    "test": "otis test -u",
    "test:watch": "otis test -u -w",
    "coverage:upload": "codecov",
    "crc": "crc"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react/prop-types": [
        "error"
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      },
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tsw38/otis": "^1.0.10",
    "axios": "0.21.1",
    "canvas": "2.6.1",
    "codecov": "3.8.1",
    "deepmerge": "4.2.2",
    "firebase-admin": "9.4.2",
    "jest-date-mock": "^1.0.8",
    "prettier": "2.2.1",
    "source-map-explorer": "2.5.1"
  },
  "jest": {
    "setupFiles": [
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__test__/jest/setup-after-env.js"
    ],
    "reporters": [
      [
        "@tsw38/otis/lib/modules/threshold-ratchet",
        {
          "tolerance": 5,
          "roundDown": true
        }
      ]
    ]
  }
}
